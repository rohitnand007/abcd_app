#content_header
  %h1#main_heading assessment_home_works
#inner-tab-menu
  %ul
    %li= link_to "#{t("web-app-theme.list", :default => "List")}", assessment_home_works_path
    %li= link_to "#{t("web-app-theme.new", :default => "New")}", new_assessment_home_work_path
    %li.inner_tab_selected=  link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_assessment_home_work_path
    %li= link_to "#{t("web-app-theme.show", :default => "Show")}", assessment_home_work_path(@assessment_home_work)
#data
  %table{width: '100%'}
    %tr
      %td
        %div
          #application-list
            = render "form"

-#.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to "#{t("web-app-theme.list", :default => "List")}", assessment_home_work_path
      %li= link_to "#{t("web-app-theme.new", :default => "New")}", new_assessment_home_work_path
      %li.active= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_assessment_home_work_path
  .content
    %h2.title
      = t("web-app-theme.edit", :default => "Edit")
      assessment_home_work.rb
    .inner
      = render "form"
-#- content_for :sidebar, render(:partial => 'shared/sidebar')
