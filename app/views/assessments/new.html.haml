#content_header
  -#%h1#main_heading Assessments
  #breadcrumbs
#inner-tab-menu
  %ul
    %li= link_to "#{t("web-app-theme.list", :default => "My Published Assessments")}", quizzes_myquizzes_path
    %li= link_to "#{t("web-app-theme.new", :default => "My Unpublished Assessments")}", quizzes_myunpublishedquizzes_path
    %li= link_to "#{t("web-app-theme.new", :default => "All Assessments")}", quizzes_path
#data
  %table{width: '100%'}
    %tr
      %td
        %div
          #application-list
            = render "form"

-#.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to "#{t("web-app-theme.list", :default => "List")}", assessments_path
      %li.active= link_to "#{t("web-app-theme.new", :default => "New")}", new_assessment_path
  .content
    %h2.title
      = t("web-app-theme.new", :default => "New")
      Assessments
    .inner
      = render "form"
-#- content_for :sidebar, render(:partial => 'shared/sidebar')
