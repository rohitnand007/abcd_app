#content_header
  %h1#main_heading Create Profile
-##inner-tab-menu
  %ul
    %li= link_to "#{t("web-app-theme.list", :default => "List")}", profiles_path
    %li.inner_tab_selected=  link_to "#{t("web-app-theme.new", :default => "New")}", new_profile_path
#data
  %table{width: '100%'}
    %tr
      %td
        %div
          #application-list
            = render "form"

-#.block
  .secondary-navigation
    %ul.wat-cf
      %li.first=# link_to "#{t("web-app-theme.list", :default => "List")}", profiles_path
      %li.active=# link_to "#{t("web-app-theme.new", :default => "New")}", new_profile_path
  .content
    %h2.title
      = t("web-app-theme.new", :default => "New")
      Profile
    .inner
      = render "form"
-#- content_for :sidebar, render(:partial => 'shared/sidebar')
