<%unless params[:user_ids].split(',').empty?%>
<%params[:user_ids].split(',').each do |user_id|%>
<%is_enrolled = User.find(user_id).try(:is_enrolled)%>
<%span_class = (is_enrolled) ? "green_label" : "red_label"%>
<%status =  (is_enrolled) ? "Enrolled" : "De-Enrolled"%>
$("#enroll_status_<%=user_id%>").html("<span class=<%=span_class%> ><%=status%></span> ");
<%end%>
<%end%>