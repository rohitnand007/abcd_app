#content_header
  %h1#main_heading Teachers
#inner-tab-menu
  %ul
    %li=link_to  "#{t("web-app-theme.list", :default => "List")}", teachers_path
    - unless current_user.is? 'EO'
      %li.inner_tab_selected=link_to link_to "#{t("web-app-theme.new", :default => "New")}", new_teacher_path
#data
  %table{width: '100%'}
    %tr
      %td
        %div
          #application-list
            = render "form"

-#.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to "#{t("web-app-theme.list", :default => "List")}", teachers_path
      %li.active= link_to "#{t("web-app-theme.new", :default => "New")}", new_teacher_path
  .content
    %h2.title
      = t("web-app-theme.new", :default => "New")
      Teacher
    .inner
      = render "form"
-#- content_for :sidebar, render(:partial => 'shared/sidebar')
