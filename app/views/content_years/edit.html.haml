#content_header
  %h1#main_heading Content years
#inner-tab-menu
  %ul
    %li=link_to  "#{t("web-app-theme.list", :default => "List")}",content_years_path
    %li.inner_tab_selected= link_to "#{t("web-app-theme.edit", :default => "Edit")}",edit_content_year_path
    %li=link_to link_to "#{t("web-app-theme.new", :default => "New")}", new_content_year_path
    %li=  link_to "#{t("web-app-theme.show", :default => "Show")}",content_year_path
#data
  %table{width: '100%'}
    %tr
      %td
        %div
          #application-list
            = render "form"

-#.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to "#{t("web-app-theme.list", :default => "List")}", content_years_path
      %li= link_to "#{t("web-app-theme.new", :default => "New")}", new_content_year_path
      %li.active= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_content_year_path
  .content
    %h2.title
      = t("web-app-theme.edit", :default => "Edit")
      ContentYears
    .inner
      = render "form"
-#- content_for :sidebar, render(:partial => 'shared/sidebar')
