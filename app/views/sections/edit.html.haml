#content_header
  %h1#main_heading Sections
#inner-tab-menu
  %ul
    %li=link_to  "#{t("web-app-theme.list", :default => "List")}", sections_path
    %li.inner_tab_selected= link_to "#{t("web-app-theme.edit", :default => "Edit")}",edit_section_path
    - unless current_user.is? 'EO'
      %li=link_to link_to "#{t("web-app-theme.new", :default => "New")}", new_section_path
    %li=  link_to "#{t("web-app-theme.show", :default => "Show")}",section_path
#data
  %table{width: '100%'}
    %tr
      %td
        %div
          #application-list
            = render "form"

