#content_header
  %h1#main_heading Sections Overview
#inner-tab-menu
  - unless current_user.is? "ET"
    %ul
      %li.inner_tab_selected=  link_to "#{t("web-app-theme.list", :default => "List")}", sections_path
      - unless current_user.is? 'EO'
        %li= link_to "#{t("web-app-theme.new", :default => "New")}", new_section_path
#data
  %table{width: '100%'}
    %tr
      %td
        = render 'sections'