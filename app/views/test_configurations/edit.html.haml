#content_header
  %h1#main_heading Test Configurations
#inner-tab-menu
  %ul
    %li=link_to  "#{t("web-app-theme.list", :default => "List")}", test_configurations_path
    %li.inner_tab_selected= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_test_configuration_path
    %li=link_to link_to "#{t("web-app-theme.new", :default => "New")}", new_test_configuration_path
    %li=  link_to "#{t("web-app-theme.show", :default => "Show")}",test_configuration_path(@test_configuration)
#data
  %table{width: '100%'}
    %tr
      %td
        %div
          #application-list
            = render "form"
-#- content_for :sidebar, render(:partial => 'shared/sidebar')




-#.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to "#{t("web-app-theme.list", :default => "List")}", test_configurations_path
      %li=# link_to "#{t("web-app-theme.new", :default => "New")}", new_test_configuration_path
      %li.active= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_test_configuration_path
  .content
    %h2.title
      = t("web-app-theme.edit", :default => "Edit")
      Test Configuration
    .inner
      = render "form"
-#- content_for :sidebar, render(:partial => 'shared/sidebar')
