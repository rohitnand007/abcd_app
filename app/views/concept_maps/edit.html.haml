#content_header
  %h1#main_heading concept_maps
#inner-tab-menu
  %ul
    %li= link_to "#{t("web-app-theme.list", :default => "List")}", concept_maps_path
    %li= link_to "#{t("web-app-theme.new", :default => "New")}", new_concept_map_path
    %li.inner_tab_selected=  link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_concept_map_path
    %li= link_to "#{t("web-app-theme.show", :default => "Show")}", concept_map_path(@concept_map)
#data
  %table{width: '100%'}
    %tr
      %td
        %div
          #application-list
            = render "form"

-#.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to "#{t("web-app-theme.list", :default => "List")}", concept_map_path
      %li= link_to "#{t("web-app-theme.new", :default => "New")}", new_concept_map_path
      %li.active= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_concept_map_path
  .content
    %h2.title
      = t("web-app-theme.edit", :default => "Edit")
      concept_map.rb
    .inner
      = render "form"
-#- content_for :sidebar, render(:partial => 'shared/sidebar')
