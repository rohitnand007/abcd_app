#content_header
  %h1#main_heading Users
#inner-tab-menu
  %ul
    %li=link_to  "#{t("web-app-theme.list", :default => "List")}", users_path
    %li.inner_tab_selected= link_to "#{t("web-app-theme.edit", :default => "Edit")}",edit_user_path
    - unless current_user.is? 'EO'
      %li=link_to link_to "#{t("web-app-theme.new", :default => "New")}", new_user_path
    %li=  link_to "#{t("web-app-theme.show", :default => "Show")}",user_path
#data
  %table{width: '100%'}
    %tr
      %td
        %div
          #application-list
            = render "edit_form"


-#.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to "#{t("web-app-theme.list", :default => "List")}", users_path
      %li= link_to "#{t("web-app-theme.new", :default => "New")}", new_user_path
      %li.active= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_user_path
  .content
    %h2.title
      = t("web-app-theme.edit", :default => "Edit")
      User
    .inner
      = render "edit_form"
-#- content_for :sidebar, render(:partial => 'shared/sidebar')
