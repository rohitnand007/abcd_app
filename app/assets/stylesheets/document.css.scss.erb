body {
    color: #111111;
    font-family: HelveticaNeueLTStd55Roman,Arial,sans-serif;
    background-color:#9acd32;
    background-repeat: no-repeat;
    background-position: 200px 280px;
}
.details .even td {
    padding: 10px;
}
.details .even {
    background: none repeat scroll 0 0 #f9f9f9;
}
.details .right {
    color: #000000;
    text-align: left;
    width: 350px;
}
.details .left {
    color: #60C8CD;
}
.details .odd {
    background: none repeat scroll 0 0 #FFFFFF;
}
.details .odd td {
    padding: 10px;
}
.details .left {
    text-align: right;
    width: 350px;
}
#data {
    margin-top: 10px;
    min-height: 70%;
    color: blue;
    background-color: ffffff;
    width: 100%;
}
#application-list {
    margin-left: 5px;
}
